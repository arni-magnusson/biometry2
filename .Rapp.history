#' @examples#
#' 	#
#' model <- lm(dist~speed, data=cars)#
#' plot.lm(model)#
#
plot.lm <- function(m) {#
#
	layout(matrix(c(1, 1, 2, 3,#
                1, 1, 4, 5), #
		nrow=2, #
		byrow=TRUE),#
		widths = c(2.5,2.5,2.5,2.5))#
	par(pty="s")#
	plot(mod$model[,1]~mod$model[,2])#
	abline(mod)#
	plot(mod, which = 1)#
	plot(mod, which = 2)#
	plot(mod, which = 3)#
	plot(mod, which = 4)#
}
